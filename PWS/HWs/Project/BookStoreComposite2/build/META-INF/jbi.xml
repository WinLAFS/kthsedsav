<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://enterprise.netbeans.org/bpel/BookStoreBPEL2/bookStoreBPEL2" xmlns:ns2="BookStoreComposite2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
    <service-assembly>
        <identification>
            <name>BookStoreComposite2</name>
            <description>Represents the Service Assembly of BookStoreComposite2</description>
        </identification>
        <service-unit>
            <identification>
                <name>BookStoreComposite2-BookStoreBPEL2</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>BookStoreBPEL2.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>BookStoreComposite2-sun-http-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-http-binding.jar</artifacts-zip>
                <component-name>sun-http-binding</component-name>
            </target>
        </service-unit>
        <connections>
            <connection>
                <consumer endpoint-name="PublisherWSRole_partnerRole" service-name="ns1:PublisherLink"/>
                <provider endpoint-name="casaPort2" service-name="ns2:BookStoreComposite2Service3"/>
            </connection>
            <connection>
                <consumer endpoint-name="casaPort3" service-name="ns2:BookStoreComposite2Service4"/>
                <provider endpoint-name="PublisherWSCallBackRole_myRole" service-name="ns1:PublisherCallbackLink"/>
            </connection>
            <connection>
                <consumer endpoint-name="casaPort5" service-name="ns2:BookStoreComposite2Service6"/>
                <provider endpoint-name="PublisherUSAWSCallBackRole_myRole" service-name="ns1:PublisherUSALink"/>
            </connection>
            <connection>
                <consumer endpoint-name="PublisherUSAWSRole_partnerRole" service-name="ns1:PublisherUSALink2"/>
                <provider endpoint-name="casaPort4" service-name="ns2:BookStoreComposite2Service5"/>
            </connection>
            <connection>
                <consumer endpoint-name="casaPort1" service-name="ns2:BookStoreComposite2Service1"/>
                <provider endpoint-name="ClientCompositePortTypeRole_myRole" service-name="ns1:PartnerLink1"/>
            </connection>
            <connection>
                <consumer endpoint-name="AuthorizationWSRole_partnerRole" service-name="ns1:AuthorizationPartnerLink"/>
                <provider endpoint-name="Authorization" service-name="ns2:BookStoreComposite2Service2"/>
            </connection>
        </connections>
    </service-assembly>
</jbi>
