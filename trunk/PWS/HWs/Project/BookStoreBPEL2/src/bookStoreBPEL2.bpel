<?xml version="1.0" encoding="UTF-8"?>
<process
    name="bookStoreBPEL2"
    targetNamespace="http://enterprise.netbeans.org/bpel/BookStoreBPEL2/bookStoreBPEL2"
    xmlns:tns="http://enterprise.netbeans.org/bpel/BookStoreBPEL2/bookStoreBPEL2"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:sxed2="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2" xmlns:ns0="http://enterprise.netbeans.org/bpel/WizardCorrelationProperties">
    <import namespace="http://j2ee.netbeans.org/wsdl/BookStoreBPEL2/ClientComposite" location="ClientComposite.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/AuthorizationWSServiceWrapper" location="AuthorizationWSServiceWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://authorization/" location="BookStore2/wsdl/AuthorizationWSService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/PublisherWSCallBackServiceWrapper" location="PublisherWSCallBackServiceWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://bookpublishercallback/" location="PublisherWSCallBackService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/PublisherWSServiceWrapper" location="PublisherWSServiceWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://bookpublisher/" location="BookStore2/wsdl/PublisherWSService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/WizardCorrelationProperties" location="WizardCorrelationProperties.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/PublisherUSAWSCallBackServiceWrapper" location="PublisherUSAWSCallBackServiceWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://bookpublisherUSAcallback/" location="PublisherUSAWSCallBackService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/PublisherUSAWSServiceWrapper" location="PublisherUSAWSServiceWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://bookpublisherUSA/" location="BookStore2/wsdl/PublisherUSAWSService.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="AuthorizationPartnerLink" xmlns:tns="http://enterprise.netbeans.org/bpel/AuthorizationWSServiceWrapper" partnerLinkType="tns:AuthorizationWSLinkType" partnerRole="AuthorizationWSRole"/>
        <partnerLink name="PublisherLink" xmlns:tns="http://enterprise.netbeans.org/bpel/PublisherWSServiceWrapper" partnerLinkType="tns:PublisherWSLinkType" partnerRole="PublisherWSRole"/>
        <partnerLink name="PublisherUSALink2" xmlns:tns="http://enterprise.netbeans.org/bpel/PublisherUSAWSServiceWrapper" partnerLinkType="tns:PublisherUSAWSLinkType" partnerRole="PublisherUSAWSRole"/>
        <partnerLink name="PartnerLink1" xmlns:tns="http://j2ee.netbeans.org/wsdl/BookStoreBPEL2/ClientComposite" partnerLinkType="tns:ClientComposite" myRole="ClientCompositePortTypeRole"/>
        <partnerLink name="PublisherCallbackLink" xmlns:tns="http://enterprise.netbeans.org/bpel/PublisherWSCallBackServiceWrapper" partnerLinkType="tns:PublisherWSCallBackLinkType" myRole="PublisherWSCallBackRole"/>
        <partnerLink name="PublisherUSALink" xmlns:tns="http://enterprise.netbeans.org/bpel/PublisherUSAWSCallBackServiceWrapper" partnerLinkType="tns:PublisherUSAWSCallBackLinkType" myRole="PublisherUSAWSCallBackRole"/>
    </partnerLinks>
    <variables>
        <variable name="FindBooksOutUSA" xmlns:tns="http://bookpublisherUSA/" messageType="tns:findBooksResponse"/>
        <variable name="FindBooksInUSA" xmlns:tns="http://bookpublisherUSA/" messageType="tns:findBooks"/>
        <variable name="FindBooksOut" xmlns:tns="http://bookpublisher/" messageType="tns:findBooksResponse"/>
        <variable name="FindBooksIn" xmlns:tns="http://bookpublisher/" messageType="tns:findBooks"/>
        <variable name="BookOrderDoneIn" xmlns:tns="http://bookpublishercallback/" messageType="tns:bookOrderDone"/>
        <variable name="OrderBookPurchaseOut" xmlns:tns="http://bookpublisher/" messageType="tns:orderBookPurchaseResponse"/>
        <variable name="OrderBookPurchaseIn" xmlns:tns="http://bookpublisher/" messageType="tns:orderBookPurchase"/>
        <variable name="LoginOut" xmlns:tns="http://authorization/" messageType="tns:loginResponse"/>
        <variable name="LoginIn" xmlns:tns="http://authorization/" messageType="tns:login"/>
        <variable name="ClientCompositeOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/BookStoreBPEL2/ClientComposite" messageType="tns:ClientCompositeOperationResponse"/>
        <variable name="ClientCompositeOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/BookStoreBPEL2/ClientComposite" messageType="tns:ClientCompositeOperationRequest"/>
    </variables>
    <correlationSets>
        <correlationSet name="wzrd_set_Invoke1_Receive2" properties="ns0:wzrd_prop_arg0_ISBN"/>
    </correlationSets>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="PartnerLink1" operation="ClientCompositeOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/BookStoreBPEL2/ClientComposite" portType="tns:ClientCompositePortType" variable="ClientCompositeOperationIn"></receive>
        <assign name="AssignLogin">
            <copy>
                <from variable="ClientCompositeOperationIn" part="username"/>
                <to>$LoginIn.parameters/arg0</to>
            </copy>
            <copy>
                <from variable="ClientCompositeOperationIn" part="password"/>
                <to>$LoginIn.parameters/arg1</to>
            </copy>
        </assign>
        <invoke name="InvokeLogin" partnerLink="AuthorizationPartnerLink" operation="login" xmlns:tns="http://authorization/" portType="tns:AuthorizationWS" inputVariable="LoginIn" outputVariable="LoginOut"/>
        <if name="If1">
            <condition>$LoginOut.parameters/return/username = 'NOTLOGGED'</condition>
            <assign name="AssignNotLogged">
                <copy>
                    <from>'Not logged'</from>
                    <to variable="ClientCompositeOperationOut" part="part1"/>
                </copy>
            </assign>
            <else>
                <sequence name="Sequence1">
                    <flow name="Flow1">
                        <sequence name="Sequence2">
                            <assign name="Assign2">
                                <copy>
                                    <from variable="ClientCompositeOperationIn" part="bookSearchTerm"/>
                                    <to>$FindBooksIn.parameters/arg0</to>
                                </copy>
                                <copy>
                                    <from variable="ClientCompositeOperationIn" part="bookSearchTerm"/>
                                    <to>$FindBooksIn.parameters/arg1</to>
                                </copy>
                                <copy>
                                    <from variable="ClientCompositeOperationIn" part="bookSearchTerm"/>
                                    <to>$FindBooksIn.parameters/arg2</to>
                                </copy>
                            </assign>
                            <invoke name="Invoke2" partnerLink="PublisherLink" operation="findBooks" xmlns:tns="http://bookpublisher/" portType="tns:PublisherWS" inputVariable="FindBooksIn" outputVariable="FindBooksOut"/>
                        </sequence>
                        <sequence name="Sequence3">
                            <assign name="Assign3">
                                <copy>
                                    <from variable="ClientCompositeOperationIn" part="bookSearchTerm"/>
                                    <to>$FindBooksInUSA.parameters/arg0</to>
                                </copy>
                                <copy>
                                    <from variable="ClientCompositeOperationIn" part="bookSearchTerm"/>
                                    <to>$FindBooksInUSA.parameters/arg1</to>
                                </copy>
                                <copy>
                                    <from variable="ClientCompositeOperationIn" part="bookSearchTerm"/>
                                    <to>$FindBooksInUSA.parameters/arg2</to>
                                </copy>
                            </assign>
                            <invoke name="Invoke3" partnerLink="PublisherUSALink2" operation="findBooks" xmlns:tns="http://bookpublisherUSA/" portType="tns:PublisherUSAWS" inputVariable="FindBooksInUSA" outputVariable="FindBooksOutUSA"/>
                        </sequence>
                    </flow>
                    <assign name="AssignLogged">
                        <copy>
                                <from>'logged :-)'</from>
                                    <to variable="ClientCompositeOperationOut" part="part1"/>
                            </copy>
                        <copy>
                            <from>'111'</from>
                            <to>$OrderBookPurchaseIn.parameters/arg0</to>
                        </copy>
                    </assign>
                    <invoke name="Invoke1" partnerLink="PublisherLink" operation="orderBookPurchase" xmlns:tns="http://bookpublisher/" portType="tns:PublisherWS" inputVariable="OrderBookPurchaseIn" outputVariable="OrderBookPurchaseOut">
                        <correlations>
                            <correlation set="wzrd_set_Invoke1_Receive2" initiate="yes" pattern="request"/>
                        </correlations>
                    </invoke>
                    <receive name="Receive2" partnerLink="PublisherCallbackLink" operation="bookOrderDone" xmlns:tns="http://bookpublishercallback/" portType="tns:PublisherWSCallBack" createInstance="no" variable="BookOrderDoneIn">
                        <correlations>
                            <correlation set="wzrd_set_Invoke1_Receive2" initiate="no"/>
                        </correlations>
                    </receive>
                    <assign name="Assign1">
                        <copy>
                            <from>$BookOrderDoneIn.parameters/arg0/bookInfo/ISBN</from>
                            <to variable="ClientCompositeOperationOut" part="part1"/>
                        </copy>
                    </assign>
                </sequence>
            </else>
        </if>
        <reply name="Reply1" partnerLink="PartnerLink1" operation="ClientCompositeOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/BookStoreBPEL2/ClientComposite" portType="tns:ClientCompositePortType" variable="ClientCompositeOperationOut"/>
    </sequence>
</process>






