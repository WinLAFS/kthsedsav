<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns0="http://enterprise.netbeans.org/bpel/BookStoreBPEL2/bookStoreBPEL2" xmlns:ns1="http://authorization/" xmlns:ns2="http://bookpublisher/" xmlns:ns3="http://j2ee.netbeans.org/wsdl/BookStoreBPEL2/ClientComposite" xmlns:ns4="http://bookpublishercallback/" xmlns:ns5="http://www.sun.com/jbi/descriptor/service-unit" version="1.0">
    <services binding-component="false">
        <provides endpoint-name="ClientCompositePortTypeRole_myRole" interface-name="ns3:ClientCompositePortType" service-name="ns0:PartnerLink1">
            <ns5:display-name>PartnerLink1</ns5:display-name>
            <ns5:process-name>bookStoreBPEL2</ns5:process-name>
            <ns5:file-path>bookStoreBPEL2.bpel</ns5:file-path>
        </provides>
        <provides endpoint-name="PublisherWSCallBackRole_myRole" interface-name="ns4:PublisherWSCallBack" service-name="ns0:PublisherCallbackLink">
            <ns5:display-name>PublisherCallbackLink</ns5:display-name>
            <ns5:process-name>bookStoreBPEL2</ns5:process-name>
            <ns5:file-path>bookStoreBPEL2.bpel</ns5:file-path>
        </provides>
        <consumes endpoint-name="AuthorizationWSRole_partnerRole" interface-name="ns1:AuthorizationWS" service-name="ns0:AuthorizationPartnerLink">
            <ns5:display-name>AuthorizationPartnerLink</ns5:display-name>
            <ns5:process-name>bookStoreBPEL2</ns5:process-name>
            <ns5:file-path>bookStoreBPEL2.bpel</ns5:file-path>
        </consumes>
        <consumes endpoint-name="PublisherWSRole_partnerRole" interface-name="ns2:PublisherWS" service-name="ns0:PublisherLink">
            <ns5:display-name>PublisherLink</ns5:display-name>
            <ns5:process-name>bookStoreBPEL2</ns5:process-name>
            <ns5:file-path>bookStoreBPEL2.bpel</ns5:file-path>
        </consumes>
    </services>
</jbi>
