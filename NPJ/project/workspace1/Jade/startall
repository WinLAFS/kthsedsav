echo "Starting MISWrapper"
ant testG4A-run-MISWrapper
result=$?
if [ $result -eq 0 ]; then
    echo "Executed MISWrapper "
else
    echo "MISWrapper execution failed "
	exit 1
fi
echo "Starting CASWrapper"
ant testG4A-run-CASWrapper
result=$?
if [ $result -eq 0 ]; then
    echo "Executed CASWrapper "
else
    echo "CASWrapper execution failed "
	exit 1
fi
echo "Starting CASInitiator"
ant testG4A-run-CASInitiator
result=$?
if [ $result -eq 0 ]; then
    echo "Executed CASInitiator Wrapper "
else
    echo "CASInitiator execution failed "
	exit 1
fi

echo "Done initializing market"

echo "Starting ResourceSeller"
ant testG4A-run-ResourceSeller
echo "Done with resource seller "

#echo "Starting ResourceBuyer"
#ant testG4A-run-ResourceBuyer
#echo "Done with resource buyer "



