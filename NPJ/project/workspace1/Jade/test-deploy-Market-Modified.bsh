source("init"); 
id=deploy("ResourceBuyerWrapper");
buyer = lookupcomp("ResourceBuyerWrapper_"+id);
lc=getitf(buyer, "lifecycle-controller");
lc.startFc();
buyeritf=getitf(buyer, "buyer");

id=deploy("ReservationManagerWrapper");
c = lookupcomp("ReservationManagerWrapper_"+id);
bc=getitf(c, "binding-controller");
bc.bindFc("buyer", buyeritf);
lc=getitf(c, "lifecycle-controller");
lc.startFc();

id=deploy("ReservationManagerWrapperClient");
c = lookupcomp("ReservationManagerWrapperClient_"+id);
lc=getitf(c, "lifecycle-controller");
lc.startFc();
i=getitf(c, "r");
i.run();
